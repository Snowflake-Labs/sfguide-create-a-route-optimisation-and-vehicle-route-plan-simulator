# OpenRouteService Native App Installer Configuration
# Edit these values to customize your installation

# Snowflake CLI connection name
# This should match a connection you've created with: snow connection add
CONNECTION_NAME=ors_connection

# Database name for the ORS setup
DATABASE_NAME=OPENROUTESERVICE_SETUP

# Default map file (San Francisco)
# You can replace this with your own .osm.pbf file
DEFAULT_MAP_FILE=SanFrancisco.osm.pbf

# ORS Configuration file
CONFIG_FILE=ors-config.yml

# Application identifiers (usually don't need to change these)
APP_PACKAGE_NAME=openrouteservice_native_app_pkg
APP_NAME=openrouteservice_native_app

# Docker image versions (update if you modify the containers)
ORS_IMAGE_VERSION=v9.0.0
VROOM_IMAGE_VERSION=v1.0.1
GATEWAY_IMAGE_VERSION=v0.5.6
DOWNLOADER_IMAGE_VERSION=v0.0.3

# Compute pool settings
# AUTO_SUSPEND_MINUTES: How long before services auto-suspend (240 = 4 hours)
AUTO_SUSPEND_MINUTES=240

# Memory settings for OpenRouteService (for large maps)
# Increase this value if you're processing large map files
ORS_JAVA_MEMORY=2g
